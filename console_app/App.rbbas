#tag ClassProtected Class AppInherits ConsoleApplication	#tag Event		Function Run(args() as String) As Integer		  		  dim results as RealTestResults = RealTestModule.ExecuteTests(ClassRegistry.ClassList, AddressOf IsRegressionTestClass)		  print results.FormatAsText		  		End Function	#tag EndEvent	#tag Method, Flags = &h21		Private Shared Function IsRegressionTestClass(c as RbClass) As Boolean		  return c.AttributeValue("RegressionTestClass")		End Function	#tag EndMethod	#tag ViewBehavior	#tag EndViewBehaviorEnd Class#tag EndClass